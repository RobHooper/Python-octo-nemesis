#!/usr/bin/python2.7
import random
import string
#print(random.random())
rrange = random.sample(range(25),20)
lrange = string.lowercase[:26]
anotherr = []
#random call on the list of letters
#print "".join(lrange)

for number in rrange:
	print(lrange[number])
	anotherr += lrange[number]

print anotherr
print "".join(anotherr)

print rrange
print lrange





